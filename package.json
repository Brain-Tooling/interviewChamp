{
  "name": "interviewchamp",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev:client": "vite --port 3000",
    "dev:server": "tsc-watch --project tsconfig.node.json --onSuccess \"node dist/server.js\"",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "run-db-script": "npx ts-node --project tsconfig-for-scripts.json server/database/runDbScript.ts",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "googleapis": "^118.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.10.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.3.2",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.11.1",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/passport": "^1.0.12",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-ts": "^1.3.2-1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testTimeout": 60000
  }
}
